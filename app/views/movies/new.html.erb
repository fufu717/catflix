  <head>
    <title>CATFLIX</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
    <link href="https://fonts.googleapis.com/css?family=Noto+Sans+JP&display=swap" rel="stylesheet">
    <link href="jumbotron.css" rel="stylesheet">
    <link href="../example.css" rel="stylesheet">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'relord' %>
  </head>

<%= render "shared/header"%>

<%= form_with model: @movie, local: true do |form| %>
<%= render 'shared/error_messages', model: form.object %>
  <p>title</p><%= form.text_field :title %> <br>
  <%= form.collection_select :cat_id, Cat.where(user_id: current_user.id), :id, :name %>
  <p>text</p><%= form.text_area :text %> <br>
  <p>video</p><%= form.file_field :movie %> <br>
  <%= form.submit %>
<% end %>